{
  "name": "@isham/typed-home-assistant",
  "version": "0.2.7",
  "tasks": {
    "lint": "deno lint",
    "format": "deno lint --fix && deno fmt",
    "run-cli": "deno run --allow-net --allow-read --allow-write --allow-env ./src/cli/cli.ts",
    "bump": "deno run --allow-read --allow-write ./scripts/bump.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./cli": "./src/cli/cli.ts"
  }
}
